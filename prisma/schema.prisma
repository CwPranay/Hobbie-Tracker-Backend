

generator client {
  provider = "prisma-client"
  
}

datasource db {
  provider = "postgresql"
  url =env("DATABASE_URL")
}

model User {
  id String @id @default(uuid())
  name String 
  email String @unique
  password String
  createdAt DateTime @default(now())

  habits Habit[]
  completions HabitCompletion[]
  followers Follow[] @relation("followers")
  following Follow[] @relation("following")
}
